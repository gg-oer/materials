{
    "title": "GISでみる地形オープン教材",
    "author": "h.yamauchi",
    "gitbook": ">= 3.0.0",
    "language": "ja",
    "root": ".",
    "plugins": [
        "-sharing",
        "-sharing-plus",
        "splitter",
        "collapsible-menu",
        "creativecommons",
        "favicon"
    ],
    "pluginsConfig": {
      "sharing": {
          "douban": false,
          "facebook": false,
          "google": false,
          "hatenaBookmark": false,
          "instapaper": false,
          "line": false,
          "linkedin": false,
          "messenger": false,
          "pocket": false,
          "qq": false,
          "qzone": false,
          "stumbleupon": false,
          "twitter": true,
          "viber": false,
          "vk": false,
          "weibo": false,
          "whatsapp": false,
          "all": ["facebook", "google"]
      },
    "favicon":{
        "shortcut": "./img/favicon.ico",
        "bookmark": "./img/favicon.ico",
        "appleTouch": "./img/apple-touch-icon.png",
        "appleTouchMore": {
            "120x120": "./img/favicon.ico",
            "180x180": "./img/favicon.ico"
        }
    }
},
    "styles": {
        "website": "styles/website.css"
    }
}
